<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.lab.musicdatabasefx.AlbumsController">
   <children>
      <TableView fx:id="table" layoutX="20.0" layoutY="20.0" prefHeight="300.0" prefWidth="560.0" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
        <columns>
          <TableColumn fx:id="titleColumn" prefWidth="112.0" text="Title" />
          <TableColumn fx:id="releaseYearColumn" prefWidth="112.0" text="Release year" />
            <TableColumn fx:id="genreColumn" prefWidth="112.0" text="Genre" />
            <TableColumn fx:id="numberOfTracksColumn" prefWidth="112.0" text="Number of tracks" />
            <TableColumn fx:id="ratingColumn" prefWidth="112.0" text="Rating" />
        </columns>
      </TableView>
      <Button fx:id="addButton" onAction="#addNewAlbum" layoutX="300.0" layoutY="345.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="Add" AnchorPane.bottomAnchor="25.0" AnchorPane.rightAnchor="40.0" />
      <Button fx:id="editButton" layoutX="174.0" layoutY="348.0" mnemonicParsing="false" onAction="#editAlbum" prefHeight="30.0" prefWidth="60.0" text="Edit" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="150.0" />
      <Button fx:id="rateButton" layoutX="174.0" layoutY="348.0" mnemonicParsing="false" onAction="#rateAlbum" prefHeight="30.0" prefWidth="60.0" text="Rate" AnchorPane.bottomAnchor="25.0" AnchorPane.rightAnchor="150.0" />
      <Button fx:id="tracksButton" layoutX="20.0" layoutY="348.0" mnemonicParsing="false" onAction="#openTracks" prefHeight="30.0" prefWidth="60.0" text="Tracks" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="40.0" />
      <Button fx:id="refreshButton" layoutX="20.0" layoutY="348.0" mnemonicParsing="false" onAction="#refresh" prefHeight="30.0" prefWidth="60.0" text="Refresh" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="270.0" AnchorPane.rightAnchor="270.0" />
   </children>
</AnchorPane>
